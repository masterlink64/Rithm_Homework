{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "title": {
      "$id": "/properties/title",
      "type": "string",
      "title": "The Title Schema ",
      "default": "",
      "examples": ["Scientist III"]
    },
    "salary": {
      "$id": "/properties/salary",
      "type": "integer",
      "title": "The Salary Schema ",
      "default": 0,
      "examples": [500000]
    },
    "equity": {
      "$id": "/properties/equity",
      "type": "integer",
      "title": "The Equity Schema ",
      "default": 0,
      "examples": [700]
    },
    "company_id": {
      "$id": "/properties/company_id",
      "type": "integer",
      "title": "The Company_id Schema ",
      "default": 0,
      "examples": [700]
    }
  },
  "required": ["title", "company_id"]
}
