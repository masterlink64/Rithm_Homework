{
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "first_name": {
      "$id": "/properties/first_name",
      "type": "string",
      "title": "The First_name Schema ",
      "default": "",
      "examples": [
        "Yoshi"
      ]
    },
    "last_name": {
      "$id": "/properties/last_name",
      "type": "string",
      "title": "The Last_name Schema ",
      "default": "",
      "examples": [
        "World"
      ]
    },
    "email": {
      "$id": "/properties/email",
      "type": "string",
      "title": "The Email Schema ",
      "default": "",
      "examples": [
        "yoshi@gmail.com"
      ]
    },
    "photo": {
      "$id": "/properties/photo",
      "type": "string",
      "title": "The Photo Schema ",
      "default": "",
      "examples": [
        "yoshi.jpg"
      ]
    },
    "company_id": {
      "$id": "/properties/company_id",
      "type": "integer",
      "title": "The Company_id Schema ",
      "default": 0,
      "examples": [
        2
      ]
    },
    "password": {
      "$id": "/properties/password",
      "type": "string",
      "title": "The Password Schema ",
      "default": "",
      "examples": [
        "Yoshi"
      ]
    },
    "username": {
      "$id": "/properties/username",
      "type": "string",
      "title": "The Username Schema ",
      "default": "",
      "examples": [
        "Yoshi3"
      ]
    }
  },
  "required": [
    "first_name",
    "last_name",
    "email",
    "password",
    "username"
  ]
}